# Production Environment Variables
NODE_ENV=production

# Database
DATABASE_URL="postgresql://ayinel_prod:secure_password@railway-postgres:5432/ayinel_prod?schema=public"

# JWT
JWT_SECRET="your_super_secure_jwt_secret_here_change_this"
JWT_EXPIRES_IN="7d"

# CORS
CORS_ORIGIN="https://ayinel.com,https://www.ayinel.com"

# Redis
REDIS_URL="redis://railway-redis:6379"

# File Upload
MAX_FILE_SIZE=104857600
UPLOAD_PATH="/uploads"

# Media Services
CLOUDFLARE_STREAM_TOKEN="your_cloudflare_token"
MUX_TOKEN_ID="your_mux_token_id"
MUX_TOKEN_SECRET="your_mux_token_secret"

# Payment Gateways
STRIPE_SECRET_KEY="sk_live_your_stripe_key"
STRIPE_WEBHOOK_SECRET="whsec_your_webhook_secret"
PAYPAL_CLIENT_ID="your_paypal_client_id"
PAYPAL_CLIENT_SECRET="your_paypal_client_secret"
CASHAPP_CLIENT_ID="your_cashapp_client_id"
CASHAPP_CLIENT_SECRET="your_cashapp_client_secret"
GOOGLEPAY_MERCHANT_ID="your_googlepay_merchant_id"
APPLEPAY_MERCHANT_ID="your_applepay_merchant_id"
VENMO_CLIENT_ID="your_venmo_client_id"
VENMO_CLIENT_SECRET="your_venmo_client_secret"

# Email
SMTP_HOST="smtp.gmail.com"
SMTP_PORT=587
SMTP_USER="noreply@ayinel.com"
SMTP_PASS="your_smtp_password"

# Analytics
SENTRY_DSN="your_sentry_dsn"
GOOGLE_ANALYTICS_ID="your_ga_id"

# Domain
DOMAIN="ayinel.com"
API_URL="https://api.ayinel.com"
WEB_URL="https://ayinel.com"
